{
    "eztv": {
        "url": "https://eztv.yt",
        "search": "/search/{query}",
        "list": {
            "items": "tr.forum_header_border",
            "item": {
                "title":"td.forum_thread_post:nth-of-type(2) > a.epinfo @ text",
                "url":"td.forum_thread_post:nth-of-type(1) > a @ href",
                "size": "td:nth-of-type(4) @ text",
                "time": "td:nth-of-type(5) @ text",
                "seeds": "td.forum_thread_post_end > font @ text",
                "leechers": "",
                "magnet": "td.forum_thread_post:nth-of-type(3) > a.magnet @ href"
            }
        }
    },
    "1337x": {
        "url": "https://www.1337x.am",
        "search": "/search/{query}/1/",
        "list": {
            "items": "table.table-list > tbody > tr",
            "item": {
                "title":"td.name > a:nth-of-type(2) @ text",
                "url":"td.name > a:nth-of-type(2) @ href",
                "size": "td.size @ text | re: [0-9\\.]+\\s+[A-Z]+",
                "time": "td:nth-of-type(5) @ text",
                "seeds": "td.seeds @ text",
                "leechers": "td.leeches @ text"
            },
            "next": ".pagination > ul > li.active + li > a @ href"
        },
        "item": {
            "magnet": "ul.download-links-dontblock > li > a @ href"
        }
    },
    "magnetdl": {
        "url": "https://www.magnetdl.com",
        "search": "/{query[0]}/{query}/se/desc/",
        "whitespace": "-",
        "headers": {
            "Accept": "*/*;"
        },
        "list": {
            "items": "table.download > tbody > tr:has(> td.n)",
            "item": {
                "title":"td.n > a @ text",
                "url":"td.n > a @ href",
                "size": "td:nth-of-type(6) @ text",
                "time": "td:nth-of-type(3) @ text",
                "seeds": "td.s @ text",
                "leechers": "td.l @ text",
                "magnet": "td.m > a @ href"
            },
            "next": "#pages > a:contains(Next) @ href"
        }
    },
    "ettv": {
        "url": "https://www.ettv.to",
        "search": "/torrents-search.php?search={query}",
        "list": {
            "items": "table > tr",
            "next": "div.myFrame-content > div > p > a:contains(Next) @ href",
            "item": {
                "title": "td:nth-of-type(2) > a > b @ text",
                "url": "td:nth-of-type(2) > a @ href",
                "time": "td:nth-of-type(3) @ text",
                "size": "td:nth-of-type(4) @ text",
                "seeds": "td:nth-of-type(6) > font > b @ text",
                "leechers": "td:nth-of-type(7) > font > b @ text"
            }
        },
        "item": {
            "magnet": "a.download-link @ href"
        }
    },
    "rarbg": {
        "url": "http://rarbgproxy.org/",
        "search": "/torrents.php?search={query}&order=seeders&by=DESC",
        "list": {
            "items": "table.lista2t > tbody > tr.lista2",
            "next": "#pager_links > a[title='next page'] @ href",
            "item": {
                "title": "td.lista:nth-of-type(2) > a[onmouseover] @ text",
                "url": "td.lista:nth-of-type(2) > a[onmouseover] @ href",
                "time": "td.lista:nth-of-type(3) @ text",
                "size": "td.lista:nth-of-type(4) @ text",
                "seeds": "td.lista:nth-of-type(5) > font @ text",
                "leechers": "td.lista:nth-of-type(6) @ text"
            }
        }
    }
}